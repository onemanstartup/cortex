assets_path "assets/javascript"

# Puts files under ./vendor/assets/js/components
group :vendor, :assets_path => "assets/js"  do
  asset "jquery"            # Assummes it's latests
  asset "sugar"
end

# Puts files under ./vendor/assets/js/components
group :vendor, assets_path: "assets/dev_js" do
 asset "html-inspector"
end

group :vendor, assets_path: "assets/stylesheets" do
 asset "normalize-css"
 asset "sass-bootstrap"
end


